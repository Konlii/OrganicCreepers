{
   "multipart": [
       {   "when": { "part": "lower|single" },
           "apply": { "model": "organiccreepers:creeper_plant" },
           "apply": { "model": "organiccreepers:creeper_plant", "y": 90 },
           "apply": { "model": "organiccreepers:creeper_plant", "y": 180 },
           "apply": { "model": "organiccreepers:creeper_plant", "y": 270 }
       },
       {   "when": { "part": "single", "age": "1|2|3|4|5|6|7" },
           "apply": { "model": "organiccreepers:creeper_body_shortest"  }
       },
       {   "when": { "part": "single", "age": "8|9|10|11|12|13|14|15" },
           "apply": { "model": "organiccreepers:creeper_body_shorter" }
       },
       {   "when": { "part": "lower", "age": "0|1|2|3|4|5|6|7" },
           "apply": { "model": "organiccreepers:creeper_body_short" }
       },
       {   "when": { "part": "lower", "age": "8|9|10|11|12|13|14|15" },
           "apply": { "model": "organiccreepers:creeper_body" }
       }
   ]
}