           "apply": { "model": "organiccreepers:creeper_body_short", "y": 180 },
           "apply": { "model": "organiccreepers:creeper_body_short", "y": 270 }
       },
       {   "when": { "part": "lower", "age": "2|3" },
           "apply": { "model": "organiccreepers:creeper_body" },
           "apply": { "model": "organiccreepers:creeper_body", "y": 90 },
